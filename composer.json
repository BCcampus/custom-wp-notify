{
  "name": "bccampus/custom-wp-notify",
  "description": "Opt-in, custom email notifications",
  "type": "wordpress-plugin",
  "version": "0.5.1",
  "homepage": "https://github.com/bccampus/custom-wp-notify",
  "license": "GPLv3 or later",
  "authors": [
    {
      "name": "Brad Payne",
      "homepage": "https://github.com/bdolor"
    },
    {
      "name": "Alex Paredes",
      "homepage": "https://github.com/alexpflores"
    }
  ],
  "config": {
    "platform": {
      "php": "7.0.30"
    }
  },
  "require": {
    "php": ">=7.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "humanmade/coding-standards": "^0.3"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit --configuration phpunit.xml ."
    ],
    "standards": [
      "vendor/bin/phpcs --standard=phpcs.ruleset.xml ."
    ]
  }
}
