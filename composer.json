{
  "name": "bccampus/custom-wp-notify",
  "description": "Opt-in, custom email notifications",
  "type": "wordpress-plugin",
  "version": "0.1.0",
  "homepage": "https://github.com/bccampus/custom-wp-notify",
  "license": "GPLv3 or later",
  "authors": [
    {
      "name": "Brad Payne",
      "homepage": "https://github.com/bdolor"
    },
    {
      "name": "Alex Paredes",
      "homepage": "https://github.com/alexpflores"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/bccampus/custom-rest-routes"
    }
  ],
  "extra": {
    "installer-paths": {
      "vendor/bccampus": ["bccampus/custom-rest-routes"]
    }
  },
  "require": {
    "bccampus/custom-rest-routes": "^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "humanmade/coding-standards": "^0.3.0",
    "laravel/homestead": "^7.1"
  },
  "scripts": {
    "test": [
      "@standards"
    ],
    "standards": [
      "vendor/bin/phpcs --standard=phpcs.ruleset.xml *.php ."
    ]
  }
}
